{
	"name": "daylerees/kurenai",
	"description": "Markdown document parser with metadata.",
	"keywords": ["kurenai", "markdown", "metadata", "document", "daylerees"],
	"license": "MIT",
	"autoload": {
		"psr-4": {
			"Kurenai\\": "src/"
		}
	},
	"require": {
		"php": ">=5.4.0"
	},
	"require-dev": {
		"mockery/mockery": "0.9.*",
		"phpunit/phpunit": "4.*",
		"erusev/parsedown-extra": "~0.2"
	},
	"suggest": {
		"erusev/parsedown": "Parsing markdown using erusev/parsedown"
	},
	"minimum-stability" : "dev"
}
