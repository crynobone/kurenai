{
	"name": "daylerees/kurenai",
	"description": "Markdown document parser with metadata.",
	"keywords": ["kurenai", "markdown", "metadata", "document", "daylerees"],
	"license": "MIT",
	"autoload": {
		"psr-0": {
			"Kurenai": "src/"
		}
	},
	"require": {
		"php": ">=5.4.0"
	},
	"require-dev": {
		"phpunit/phpunit": "4.*",
		"dflydev/markdown": "1.0.*",
		"erusev/parsedown-extra": "~0.2"
	},
	"suggest": {
		"dflydev/markdown": "Parsing markdown using dflydev/markdown"
	},
	"minimum-stability" : "dev"
}
